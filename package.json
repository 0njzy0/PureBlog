{
  "name": "pure-blog",
  "version": "1.0.0",
  "description": "My perfect Nuxt.js project",
  "author": "njzy",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue ./;exit 0",
    "lint:fix": "eslint --fix --ext .js,.vue ./;exit 0",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "@mdi/font": "^3.0.39",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/proxy": "^1.3.1",
    "cross-env": "^5.2.0",
    "dayjs": "^1.7.7",
    "express": "^4.16.3",
    "nuxt": "^2.3.4",
    "vuetify": "^1.3.9",
    "vuetify-loader": "^1.0.7"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.5",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6",
    "prettier": "^1.15.2",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0njzy0/PureBlog.git"
  },
